public class Cartao {
    int numCartao;
    double saldo;
    Cliente titular;

    public Cartao(int numCartao, double saldo, Cliente titular) {
        this.numCartao = numCartao;
        this.saldo = saldo;
        this.titular = titular;
    }

    public void testeCartao (){
        System.out.println("Estou na classe pai: Cart√£o");
    }

    @Override
    public String toString() {
        return "Cartao{" +
                "numCartao=" + numCartao +
                ", saldo=" + saldo +
                ", titular=" + titular +
                '}';
    }
}
